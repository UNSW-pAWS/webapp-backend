[
    {
        "name": "S3_BUCKET_LEVEL_PUBLIC_ACCESS_PROHIBITED",
        "rule": "all parameters in PublicAccessBlockConfiguration have to be true and AccessControl cannot start with 'Public'"
    },
    {
        "name": "S3_BUCKET_PUBLIC_READ_PROHIBITED",
        "rule": "AccessControl !== PublicRead && AccessControl !== PublicReadWrite"
    },
    {
        "name": "S3_BUCKET_PUBLIC_WRITE_PROHIBITED",
        "rule": "AccessControl !== PublicReadWrite"
    },
    {
        "name": "S3_ACCOUNT_LEVEL_PUBLIC_ACCESS_BLOCKS_PERIODIC",
        "rule": "all parameters in PublicAccessBlockConfiguration have to be true"
    },
    {
        "name": "S3_BUCKET_LOGGING_ENABLED",
        "rule": "LoggingConfiguration must have DestinationBucketName parameter"
    },
    {
        "name": "S3_BUCKET_SERVER_SIDE_ENCRYPTION_ENABLED",
        "rule": "default should not be altered"
    },
    {
        "name": "S3_DEFAULT_ENCRYPTION_KMS",
        "rule": "default should not be altered"
    }
]