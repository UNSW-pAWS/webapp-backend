[
    {
        "name": "RDS_AUTOMATIC_MINOR_VERSION_UPGRADE_ENABLED",
        "rule": "AutoMinorVersionUpgrade === true"
    },
    {
        "name": "RDS_INSTANCE_PUBLIC_ACCESS_CHECK",
        "rule": "PubliclyAccessible === true"
    },
    {
        "name": "RDS_LOGGING_ENABLED",
        "rule": "json(EnableCloudwatchLogsExports) length should be greater than 0"
    },
    {
        "name": "RDS_STORAGE_ENCRYPTED",
        "rule": "StorageEncrypted === true"
    }
]